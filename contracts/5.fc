() recv_internal() {
}
forall X -> tuple tpush(tuple t, X value) asm "TPUSH";
forall X -> (tuple, ()) ~tpush(tuple t, X value) asm "TPUSH";
tuple empty_tuple() asm "NIL";

(int, int) nextab (int a, int b, int n) inline
  asm "<{ TUCK ADD }>REPEAT";

(tuple) get_fib (int a, int b, int k) inline
  asm """
  DUP
  1 SETGLOB
  <{ DUP2 ADD }>REPEAT
  1 GETGLOB
  TUPLEVAR
  s2 XCHG0
  2 BLKDROP
  """;
