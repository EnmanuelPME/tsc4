() recv_internal() {
}

tuple empty_tuple() asm "NIL";

(tuple) get_fib (int n, int k) inline
  asm """
  1 SETGLOB
  -1 INT 1 INT
  s2 PUSH
  <{
  TUCK ADD
  }>REPEAT
  1 GETGLOB
  <{
  DUP2 ADD
  }>REPEAT
  1 GETGLOB
  TUPLEVAR
  SWAP
  DROP
  """;


;; testable
(tuple) fibonacci_sequence (int n, int k) method_id {
  if(k == 0) {
    return empty_tuple();
  }

  return get_fib(n, k);
}
