;; testable
(tuple) fibonacci_sequence (int n, int k) method_id {
  int a = -1;
  int b = 1;
  
  if(k == 0) {
    return empty_tuple();
  }

  (a, b) = nextab(a, b, n);

  return get_fib(a, b, k);
}
